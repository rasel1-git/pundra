#include <stdio.h>
int main() {
int i,n,a[100],pass,com,temp;
printf("\n Enter the range of array: ");
scanf("%d",&n);
printf("\n Enter the Element of array ");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("\n After traversing .....\n");
for(i=0;i<n;i++)
printf("%4d\n",a[i]);
for(pass=n-1;pass>=0;pass--)
for(com=0;com<pass;com++)
if(a[com]>a[com+1]){
temp=a[com];
a[com]=a[com+1];
a[com+1]=temp;
}
printf("\n After sorting .....\n");
for(i=0;i<n;i++)
printf("%4d\n",a[i]);
}
